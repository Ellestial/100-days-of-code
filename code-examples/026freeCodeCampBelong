
function getIndexToIns(arr, num) {
  var lowestIndex;
  // Find my place in this sorted array.
  arr.sort(function(a, b) {
    return a - b;
  });
  
  arr.map(function(el, i) {
     if(el < num) {
      lowestIndex = i + 1;
    } else if(el === num) {
      lowestIndex = i;
    }
  });
  return lowestIndex;
}

getIndexToIns([10, 20, 30, 40, 50], 30);
